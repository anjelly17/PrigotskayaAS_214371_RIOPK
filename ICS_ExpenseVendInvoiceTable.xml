			<Method>
				<Name>vendTaxValue</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets vend tax value
    /// </summary>
    /// <returns>Tax value</returns>
    public Amount vendTaxValue()
    {
        TaxGroupData    taxGroupData;
        VendTable       vendTable   = VendTable::find(this.SuppAcc);
        TaxValue        taxValue    = 0;

        if (!vendTable.InclTax)
        {
            select firstonly taxGroupData
                where taxGroupData.TaxGroup == vendTable.TaxGroup;

            taxValue = taxGroupData.showTaxValue();
        }

        return taxValue;
    }

]]></Source>
