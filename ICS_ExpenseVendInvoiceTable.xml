        ICS_ExpenseVendInvoiceApprovalStatus    _approvalStatus,
        utcdatetime                             _approvalDateTime,
        boolean                                 _isExpense)
    {
        if (_isExpense)
        {
            this.ExpenseApprovedBy          = _approvedBy;
            this.ExpenseApproval            = _approvalStatus;
            this.ExpenseApprovedDateTime    = _approvalDateTime;
        }
        else
        {
            this.BudgetApprovedBy       = _approvedBy;
            this.BudgetApproval         = _approvalStatus;
            this.BudgetApprovedDateTime = _approvalDateTime;
        }
    }

