				<Name>initFromExpenseRequest</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initialize from expense request.
    /// </summary>
    /// <param name="_request"> <c>ICS_ExpenseVendInvoiceRequest</c> instance.</param>
    /// <param name="_newRecord">Is new record?</param>
    public void initFromExpenseRequest(ICS_ExpenseVendInvoiceRequest    _request,
                                       boolean                          _newRecord = true)
    {
        AccountNum      vendAccountNum = this.getVendAccountByName();

        if (_newRecord
         || (!_newRecord 
         &&  _request.parmInvoiceId() && _request.parmInvoiceId() != this.InvoiceId))
        {
            this.InvoiceId = _request.parmInvoiceId();
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmInvoiceDate() && _request.parmInvoiceDate() != this.InvoiceDate))
        {
            this.InvoiceDate = _request.parmInvoiceDate();
        }

        if (_newRecord
         || (!_newRecord
         && _request.parmInvoiceSDI() != this.InvoiceSDI))
        {
            this.InvoiceSDI = _request.parmInvoiceSDI();
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmPathXML() && _request.parmPathXML() != this.PathXML))
        {
            this.PathXML = _request.parmPathXML();
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmPathPDF() && _request.parmPathPDF() != this.PathPDF))
        {
            this.PathPDF = _request.parmPathPDF();
        } 

        if (_newRecord
         || (!_newRecord
         &&  _request.parmAIPreview() && _request.parmAIPreview() != this.AIPreview))
        {
            this.AIPreview = _request.parmAIPreview();
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmDirPathAI() && _request.parmDirPathAI() != this.DirPathAI))
        {
            this.DirPathAI = _request.parmDirPathAI();
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmReceivingSociety() && _request.parmReceivingSociety() != this.ReceivingSociety))
        {
            this.ReceivingSociety = _request.parmReceivingSociety();
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmBarcode() && _request.parmBarcode() != this.FTP_Barcode))
        {
            this.FTP_Barcode = _request.parmBarcode(); 
        }

        if (_newRecord
         || (!_newRecord
         &&  _request.parmSupplierName() && _request.parmSupplierName() != this.SuppName))
        {
            this.SuppName   = _request.parmSupplierName();
            vendAccountNum  = this.getVendAccountByName();
        }

        if (!_newRecord
         &&  vendAccountNum != this.SuppAcc)
        {
            this.SuppAcc = vendAccountNum;
        }    
        
        if (_newRecord
         || (!_newRecord
         && _request.parmFlowType() != this.FlowType))
        {
            this.FlowType = _request.parmFlowType();
        }

        if (_newRecord
         || (!_newRecord
         && _request.parmOffsetAccount()))
        {
            this.parmOffsetAccount(_request.parmOffsetAccount(), LedgerJournalACType::Ledger);
        }        

        if (_newRecord)
        {
            this.DocumentDate   = _request.parmDocumentDate();
            this.VATNum         = _request.parmVATNum();
            this.FiscalCode     = _request.parmFiscalCode();
            this.PathPreview    = _request.parmPathPreview();  
            this.SuppAcc        = vendAccountNum;
        }  
    }

]]></Source>
