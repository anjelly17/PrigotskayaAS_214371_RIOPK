				<Name>updateDefaultDimenions</Name>
				<Source><![CDATA[
    private void updateDefaultDimenions()
    {
        ICS_ExpenseVendInvoiceLine invoiceLine;

        ttsbegin;
        select firstonly forupdate invoiceLine
            where invoiceLine.ExpenseVendInvoice == this.RecId;

        invoiceLine.initDefaultDimensions(this.MainContactWorker);

        if (!invoiceLine.validateWrite())
        {
            throw error("@SYS21628");
        }

        invoiceLine.update();
        ttscommit;
    }

