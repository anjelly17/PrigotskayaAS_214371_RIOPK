			<Method>
				<Name>getVendAccountByName</Name>
				<Source><![CDATA[
    public VendAccount getVendAccountByName()
    {
        DirPartyTable   dirPartyTable;
        VendTable       vendTable;  

        select firstonly AccountNum from vendTable
        exists join dirPartyTable
          where dirPartyTable.RecId == vendTable.Party
            &&  dirPartyTable.Name == this.SuppName;

        return vendTable.AccountNum;
    }

]]></Source>
