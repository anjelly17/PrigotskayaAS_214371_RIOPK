			<Method>
				<Name>recalcAmount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates amount on the expence vend invoice lines
    /// </summary>
    public void recalcAmount()
    {
        ICS_ExpenseVendInvoiceLine line;

        ttsbegin;

        while select forupdate line
            where line.ExpenseVendInvoice == this.RecId
        {
            line.setAmount();

            if (!line.validateWrite())
            {
                throw error("@SYS21628");
            }

            line.update();
        }

        ttscommit;
    }

]]></Source>
