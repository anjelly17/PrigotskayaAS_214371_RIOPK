			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates
    /// </summary>
    public void update()
    {
        ICS_ExpenseVendInvoice orig = this.orig();

        super();

        if (orig.SuppAcc != this.SuppAcc)
        {
            this.recalcAmount();
            this.updateDefaultDimenions();
        }

        if (orig.MainContactWorker != this.MainContactWorker)
        {
            this.updateDefaultDimenions();
        }

        if (FormDataUtil::isFormDataSource(this))
        {
            FormDataUtil::getFormDataSource(this).research(true);
        }
    }

]]></Source>
