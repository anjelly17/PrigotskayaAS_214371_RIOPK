				<Source><![CDATA[
    /// <summary>
    /// Clear Journal number from expense vendor invoice
    /// </summary>
    /// <param name = "_journalRecId">Journal refcId</param>
    public static void clearLedgerJournal(LedgerJournalTableRecId   _journalRecId)
    {
        ICS_ExpenseVendInvoice vendInvoice;

        update_recordset vendInvoice
        setting LedgerJournalTableRecId = 0,
